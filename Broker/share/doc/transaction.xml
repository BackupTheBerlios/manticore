    <script type="text/javascript" language="javascript"><![CDATA[
function runInOnlineBankingIFrame()
{
    var iFrameName = "onlineBankingIFrame";

	for(var i = 0; i < top.frames.length; i++)
	{
	   if ( top.frames[i].location == self.location &&
	        top.frames[i].name == iFrameName )
	   {
	       return true;
	   }
	}

	return false;
}

function getMode()
{
    if ( runInOnlineBankingIFrame() )
    {
        return "portal";
    }
    else
    {
        return "popup";
    }
}

function writePortalModeImage()
{
    var imageTag = "<img src=\"setPortalMode?mode=" + getMode() + "\" width=\"1px\" height=\"1px\">";
    document.write( imageTag );
}

function showPaperInfoIfNoIFrame()
{

    if ( !runInOnlineBankingIFrame() )
    {
        if ( document.getElementById( "paperInfo" ) != null )
        {
            document.getElementById("paperInfo").style.display = "inline";
        }
        if ( document.getElementById("topLogo") != null )
            document.getElementById("topLogo").style.display = "block";
    }
    
	
    
    
}

function resizeIFrameIfPortalMode()
{
    if ( runInOnlineBankingIFrame() )
    {
        iframeResizePipe();
    }
}

function resizeProgessAnimation()
{

    if ( document.getElementById("progressDisplay") == null )
        return;

    if ( runInOnlineBankingIFrame() )
    {
        document.getElementById("progressDisplay").style.left = "10px";
        document.getElementById("progressDisplay").style.top = "0px";
    }
    else
    {
        document.getElementById("progressDisplay").style.left = "480px";
        document.getElementById("progressDisplay").style.top = "20px";
    }
}

function openPortalMemberArea()
{
	

		var portalUrl="/onlinebanking-flatex/navigationMenuFormAction.do?method=openPortalMemberArea";
	
		if( window.opener != null ) 
		{
			portal = window.opener;
			portal.location.href=portalUrl;
			portal.focus();
		}
		else
		{
			portal = window.open(portalUrl,"flatex");
			portal.focus();
		}
	
}

]]></script>
