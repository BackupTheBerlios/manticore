    <script type="text/javascript" language="javascript"><![CDATA[
  function getWindowDimension()
  {
    if (window.innerHeight && window.innerWidth)
      return window.innerWidth + "x" + window.innerHeight;
    else if (document.body && document.body.clientHeight && document.body.clientWidth)
      return document.body.clientWidth + "x" + document.body.clientHeight;
    else
      return "0x0";
  }
 
  function getReferer()
  {
    var actual=document.location;
    var referer=document.referrer;
    if (actual && actual.host && referer) // actual.host=konto.flatex.de
    {
      // now build the host from the referer
      // URI https://konto.flatex.de:8888/onlinebanking-flatex
      var refererSplitted=referer.split("/");
      // in [0] -> protocol (https)
      // in [1] -> '' (//)
      // in [2] -> host
      if (refererSplitted.length >= 1 && refererSplitted[2] != actual.host)
        return encodeURIComponent(referer);
    }
 
    return "0";
  }
 
  var baseUrl = "https://flatex01.webtrekk.net/";
  var customerId= "370212450674777"; // Customer ID (FIX)
  var version= "206";  // Version des Webtrekk-Pixels (FIX)
  var pageName ="";
  
  
    pageName = "de.wfiliale.otcQuoteOrder"; // Content-ID
  
  
  var javascriptEnabled= "1"; // Javascript aktiviert (0 für deaktiviert)
  var desktopResolution = screen.width + "x" + screen.height; // Bildschirmauflösung
  var colorDepth = screen.colorDepth; // Farbtiefe in Bit
  var cookiesEnabled = navigator.cookieEnabled ? "1":"0"; // Cookies eingeschaltet (0 für deaktiviert)
  var timestamp = new Date().getTime(); // Timestamp des Client 
  var referer = getReferer(); // kein Referrer (ansonsten die codierte URL) (wenn er von extern wie Depotliga etc. kommt die Codierte URL)
  var windowDimension= getWindowDimension(); // innere Auflösung des Browserfensters
  var javaEnabled= navigator.javaEnabled() ? "1":"0"; // Java installiert (0 für nicht vorhanden)
 
  var url =baseUrl + customerId + "/wt.pl";
  url += "?p=" + version;
  url += "," + pageName;
  url += "," + javascriptEnabled;
  url += "," + desktopResolution;
  url += "," + colorDepth;
  url += "," + cookiesEnabled;
  url += "," + timestamp;
  url += "," + referer;
  url += "," + windowDimension;
  url += "," + javaEnabled;
 
  document.write("<img src=\"" + url + "\" height=\"1\" width=\"1\" alt=\"\" style=\"display:none;visibility:hidden;position:absolute;\">");
]]></script>  
